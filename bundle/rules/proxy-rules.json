      "status": "body > div.row > div > div:nth-child(1) > div.pic_txt_list > p:nth-child(5) > span"
    },
    "toc": {
      "url": "https://quanben5.com/n/%s/xiaoshuo.html",
      "item": "ul > li > a"
    },
